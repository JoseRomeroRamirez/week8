<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="AutomationTestSuite"  >
    <test name="Form Test">
        <parameter name="browser" value="firefox"/>
        <parameter name="url" value="https://demoqa.com/"/>
        <classes>
            <class name="testproject.ToolsQAWebTest">
                <methods>
                    <include name="FormTest"></include>
                </methods>
            </class>
        </classes>
    </test>
    <test name="Alert Frame Window Test">
        <parameter name="browser" value="chrome"/>
        <parameter name="url" value="https://demoqa.com/"/>
        <classes>
            <class name="testproject.ToolsQAWebTest">
                <methods>
                    <include name="AlertFrameWindowTest"></include>
                </methods>
            </class>
        </classes>
    </test>
</suite>